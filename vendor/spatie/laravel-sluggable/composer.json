{
    "name": "spatie/laravel-sluggable",
    "description": "Generate slugs when saving Eloquent models",
    "keywords": [
        "spatie",
        "laravel-sluggable"
    ],
    "homepage": "https://github.com/spatie/laravel-sluggable",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/support": "^7.0.5|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "spatie/laravel-translatable": "^4.3",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Sluggable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Sluggable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
